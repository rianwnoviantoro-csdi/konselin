{
  "name": "konselin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "yarn transpile:electron && cross-env NODE_ENV=development electron .",
    "transpile:electron": "babel src/electron --out-dir dist-electron",
    "build": "vite build",
    "dist:mac": "yarn transpile:electron && yarn build && electron-builder --mac --arm64",
    "dist:mac64": "yarn transpile:electron && yarn build && electron-builder --mac --x64",
    "dist:linux": "yarn transpile:electron && yarn build && electron-builder --linux --x64",
    "dist:win": "yarn transpile:electron && yarn build && electron-builder --win --x64",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.15",
    "@radix-ui/themes": "^3.2.1",
    "@reduxjs/toolkit": "^2.6.0",
    "@tailwindcss/vite": "^4.0.9",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.477.0",
    "match-sorter": "^8.0.0",
    "os-utils": "^0.0.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "systeminformation": "^5.25.11",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "electron": "^34.3.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "vite": "^6.2.0",
    "vite-plugin-static-copy": "^2.3.0"
  }
}
